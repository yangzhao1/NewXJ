<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="ImageTranslucentTheme" parent="AppTheme">
        <item name="textsizes">@dimen/text_small</item>
        <item name="titlesize">@dimen/text_big</item>
        <item name="textsize2">@dimen/text_middle</item>
        <item name="textsosmall">@dimen/text_sosmall</item>
	</style>
	<style name="ColorTranslucentTheme" parent="AppTheme">
        <item name="android:fitsSystemWindows">true</item>
        <item name="textsizes">@dimen/text_small</item>
        <item name="titlesize">@dimen/text_big</item>
        <item name="textsize2">@dimen/text_middle</item>
        <item name="textsosmall">@dimen/text_sosmall</item>

    </style>
    <style name="ImageTranslucentThemeTextSize_big" parent="AppTheme">
        <item name="textsizes">@dimen/text_small2</item>
        <item name="titlesize">@dimen/text_big2</item>
        <item name="textsize2">@dimen/text_middle2</item>
        <item name="textsosmall">@dimen/text_sosmall2</item>

    </style>
    <style name="ColorTranslucentThemeTextSize_big" parent="AppTheme">
        <item name="android:fitsSystemWindows">true</item>
        <item name="textsizes">@dimen/text_small2</item>
        <item name="titlesize">@dimen/text_big2</item>
        <item name="textsize2">@dimen/text_middle2</item>
        <item name="textsosmall">@dimen/text_sosmall2</item>

    </style>
    <style name="ImageTranslucentThemeTextSize_sobig" parent="AppTheme">
        <item name="textsizes">@dimen/text_small3</item>
        <item name="titlesize">@dimen/text_big3</item>
        <item name="textsize2">@dimen/text_middle3</item>
        <item name="textsosmall">@dimen/text_sosmall3</item>

    </style>
    <style name="ColorTranslucentThemeTextSize_sobig" parent="AppTheme">
        <item name="android:fitsSystemWindows">true</item>
        <item name="textsizes">@dimen/text_small3</item>
        <item name="titlesize">@dimen/text_big3</item>
        <item name="textsize2">@dimen/text_middle3</item>
        <item name="textsosmall">@dimen/text_sosmall3</item>

    </style>
    <style name="SplashTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/pop_push_up_in</item>
        <item name="android:windowExitAnimation">@anim/pop_push_down_out</item>
    </style>
    
    <!-- Application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="CustomWindowTitleBackground">
