<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/line_lc2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#f2f2f2"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:orientation="vertical"
        android:visibility="visible" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="10dp"
             >
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="21dp"
                android:layout_weight="5" >

                <ImageView
                    android:id="@+id/dian1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/huidian"
                    android:contentDescription="@string/app_name" />

                <View
                    android:id="@+id/xian1"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_centerInParent="true"
                    android:layout_toRightOf="@id/dian1"
                    android:background="#cccccc" />
            </RelativeLayout>


            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="5" >

                <ImageView
                    android:id="@+id/dian2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/huidian"
                    android:contentDescription="@string/app_name" />

                <View
                    android:id="@+id/xian2"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_centerInParent="true"
                    android:background="#cccccc" />

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="5" >

                <ImageView
                    android:id="@+id/dian3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/huidian"
                    android:contentDescription="@string/app_name" />

                <View
                    android:id="@+id/xian3"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_centerInParent="true"
                    android:background="#cccccc" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="5" >

                <ImageView
                    android:id="@+id/dian4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/huidian"
                    android:contentDescription="@string/app_name" />

                <View
                    android:id="@+id/xian4"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_centerInParent="true"
                    android:background="#cccccc" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_marginRight="20dp"
                android:layout_weight="5" >

                <ImageView
                    android:id="@+id/dian5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/huidian"
                    android:contentDescription="@string/app_name" />

                <View
                    android:id="@+id/xian5"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_toLeftOf="@id/dian5"
                    android:layout_centerInParent="true"
                    android:background="#cccccc" />

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="30dp" >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="21dp"
                android:layout_weight="5"
                android:gravity="center_horizontal|center_vertical" >

                <TextView
                    android:id="@+id/zi1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="开始"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="5"
                android:gravity="center_horizontal|center_vertical" >

                <TextView
                    android:id="@+id/zi2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="窗口受理"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_14" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="5"
                android:layout_height="match_parent"
                android:gravity="center_horizontal|center_vertical" >

                <TextView
                    android:id="@+id/zi3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="部门批示"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="5"
                android:gravity="center_horizontal|center_vertical" >

                <TextView
                    android:id="@+id/zi4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="独立收费"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="5"
                android:layout_marginRight="20dp"
                android:gravity="center_horizontal|center_vertical" >

                <TextView
                    android:id="@+id/zi5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="结束"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_14" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>